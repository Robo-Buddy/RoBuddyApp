<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="seniordesign.robuddyuserapp">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="seniordesign.robuddyuserapp.MainPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name="seniordesign.robuddyuserapp.ViewCheckedOutActivity" />
        <activity android:name="seniordesign.robuddyuserapp.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="seniordesign.robuddyuserapp.SearchResultsActivity" />
        <activity android:name="seniordesign.robuddyuserapp.ItemDetailActivity" />
        <activity android:name="seniordesign.robuddyuserapp.ItemCheckOutActivity"></activity>
    </application>

</manifest>